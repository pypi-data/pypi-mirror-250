#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules/dts-buddy/src/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules/dts-buddy/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules/dts-buddy/src/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules/dts-buddy/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/dts-buddy@0.2.5/node_modules:/home/vemonet/dev/llm/libre-chat/frontend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../dts-buddy@0.2.5/node_modules/dts-buddy/src/cli.js" "$@"
else
  exec node  "$basedir/../../../../../dts-buddy@0.2.5/node_modules/dts-buddy/src/cli.js" "$@"
fi
